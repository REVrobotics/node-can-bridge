{
    "name": "@rev-robotics/can-bridge",
    "version": "3.4.0",
    "author": "REV Robotics",
    "description": "Access CAN Data from a USB device in Node.js",
    "license": "MIT",
    "main": "dist/binding.js",
    "types": "dist/binding.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/REVrobotics/node-can-bridge.git"
    },
    "dependencies": {
        "cmake-js": "^7.3.0",
        "node-addon-api": "^6.0.0",
        "pkg-prebuilds": "^1.0.0"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.5.0",
        "@types/node": "^16.18.18",
        "adm-zip": "^0.5.10",
        "axios": "^1.3.6",
        "typescript": "^5.0.2"
    },
    "scripts": {
        "install": "pkg-prebuilds-verify binding-options.cjs || npm run build",
        "build": "node scripts/download-CanBridge.mjs && cmake-js compile && tsc",
        "prepublishOnly": "npm run build && pkg-prebuilds-copy --baseDir build/Release --source node_canbridge.node --name node_canbridge --napi_version=8",
        "pretest": "npm run build",
        "test": "echo running test && node --napi-modules test/test_binding.js"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "binary": {
        "napi_versions": [
            3
        ]
    }
}
